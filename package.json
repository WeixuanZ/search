{
  "name": "search",
  "version": "0.0.1",
  "description": "Algolia instant search integration for static blog",
  "main": "index.js",
  "author": "W Zhang",
  "license": "MIT",
  "scripts": {
    "dev": "parcel src/index.html src/search.html client.ts",
    "build": "parcel build src/index.html src/search.html client.ts --public-url /search",
    "deploy": "gh-pages --dist /dist",
    "format": "prettier --config .prettierrc.json 'src/**/*.ts' --write"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "gh-pages": "^3.1.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1",
    "sass": "^1.29.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "algoliasearch": "^4.8.2",
    "instantsearch.js": "^4.9.0"
  }
}
